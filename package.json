{
  "name": "@mangroves/jamcaa-helper",
  "version": "0.1.2",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "node ./scripts/build.js",
    "db": "node ./scripts/db.js",
    "db:destroy": "docker stop jamcaa-mysql && docker rm jamcaa-mysql",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest --testPathPattern tests/unit/.*\\.spec\\.ts --coverage",
    "test:e2e": "jest --testPathPattern tests/e2e/.*\\.spec\\.ts --coverage",
    "prepublishOnly": "npm run test && npm run build"
  },
  "files": [
    "src",
    "dist"
  ],
  "keywords": [
    "jamcaa",
    "nestjs",
    "typeorm",
    "crud",
    "mysql"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mangroves-fe/jamcaa-helper"
  },
  "author": "mangroves-fe",
  "license": "MIT",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@microsoft/api-extractor": "^7.18.1",
    "@nestjs/common": "^7.6.18",
    "@nestjs/testing": "^7.6.18",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "eslint": "^7.28.0",
    "execa": "^5.1.1",
    "fs-extra": "^10.0.0",
    "husky": "^6.0.0",
    "jest": "^27.0.4",
    "mysql2": "^2.2.5",
    "rollup": "^2.52.1",
    "ts-jest": "^27.0.3",
    "tslib": "^2.3.0",
    "typeorm": "^0.2.34",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@mangroves/field-mask": "^0.1.0"
  }
}
